generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Producto {
  id          Int       @id @default(autoincrement())
  codigo      String
  descripcion String
  cantidad    Int       @default(1)
  fotoUrl     String?
  vendedor    String
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
}
